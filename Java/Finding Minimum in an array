public class FindMin {
    public static void main(String[] args) {
        int[] arr = {18, 12, 7, 3, 1,28};
        System.out.println(min(arr));
    }
    static int min(int[] str) {
        int ans = str[0];
        for (int i = 1; i < str.length ; i++) {
            if(str[i] < ans){
                ans = str[i];
            }
        }
        return ans;
    }
}
