import java.util.Scanner;

public class Product {
    public static void main(String[] args) {
        int product = product();
        System.out.println(product);
    }

     static int product() {
         Scanner in = new Scanner(System.in);
         System.out.print("Enter two numbers: ");
         int num1 = in.nextInt();
         int num2 = in.nextInt();
         int ans = num1 * num2;
         return ans;
    }
}
