
import java.util.Scanner;

public class Addition {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        System.out.print("Enter two numbers: ");
        int num = in.nextInt();
        int num1 = in.nextInt();
        int add = num + num1;
        System.out.println("The addition of the numbers is "+ add);
    }
}
